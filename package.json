{
  "name": "gitlab-figma-plugin",
  "description": "GitLab Figma Plugin",
  "license": "MIT",
  "scripts": {
    "test": "jest tests/",
    "test:figma": "jest tests/figma/",
    "test:ui": "jest tests/ui/",
    "eslint": "eslint --ext .js,.vue,.ts .",
    "eslint-fix": "eslint --ext .js,.vue,.ts . --fix",
    "build:clean": "rm -Rf dist/ && npm ci && npm run build",
    "build": "webpack --mode=production",
    "build:develop": "webpack --mode=development",
    "build:stats": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json dist",
    "watch": "webpack --mode=development --watch",
    "watch-gdk": "GITLAB_INSTANCE_PROTOCOL='http:' webpack --mode=development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/gitlab-org/gitlab-figma-plugin.git"
  },
  "author": "GitLab <tquirk@gitlab.com>",
  "bugs": {
    "url": "https://gitlab.com/gitlab-org/gitlab-figma-plugin/issues"
  },
  "homepage": "https://gitlab.com/gitlab-org/gitlab-figma-plugin#readme",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@figma/plugin-typings": "^1.15.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-alpha.8",
    "html-inline-css-webpack-plugin": "^1.8.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.1.0",
    "jest-transform-graphql": "^2.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mock-apollo-client": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "purgecss-webpack-plugin": "^2.3.0",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.3",
    "vue-svg-loader": "^0.16.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "apollo-upload-client": "^13.0.0",
    "bootstrap": "^4.4.1",
    "figma-plugin-ds": "0.1.8",
